@model NLCS.Models.Entities.Faculty

@{
    ViewData["Title"] = "DeleteFaculty";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h3>Bạn thật sự muốn xóa?</h3>
<div>
    <h4>Khoa</h4>
    <hr />
    <dl class="row">
            <img src="@Url.Content(Model.Faculty_Image)"/>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Faculty_Id)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Faculty_Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Faculty_Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Faculty_Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Faculty_Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Faculty_Description)
        </dd>
    </dl>
    
    <form asp-action="DeleteFaculty">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        
    </form>
</div>
